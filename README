unset $(grep -v '^#' .env | sed -E 's/(.*)=.*/\1/' | xargs)
export $(grep -v '^#' .env | xargs -d '\n')

### Unset all environment variables defined in .env
`unset $(grep -v '^#' .env | sed -E 's/(.*)=.*/\1/' | xargs)`

### Set all environment variables defined in .env
`export $(grep -v '^#' .env | xargs -d '\n')`